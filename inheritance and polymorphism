
class Animal(object):
    def run(self):
        print('Animal is running...')
        
class Dog(Animal):
    def run():
        print('Dog is running...')
    def eat():
        print('Dog is eating...')
        
class Cat(Animal):
    def run(self):
        print('Cat is running...')
    pass
def run_twice(animal):
    animal.run()
    animal.run()

dog = Dog()
dog.run()
cat = Cat()
cat.run()


# 定义一个父类一个子类
